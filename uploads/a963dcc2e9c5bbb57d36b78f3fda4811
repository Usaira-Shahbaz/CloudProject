if [ ! -d "/etc/smbcredentials" ]; then
sudo mkdir /etc/smbcredentials
fi
if [ ! -f "/etc/smbcredentials/onlineazurestorage.cred" ]; then
    sudo bash -c 'echo "username=onlineazurestorage" >> /etc/smbcredentials/onlineazurestorage.cred'
    sudo bash -c 'echo "password=2AIUbyXNVp+fratQU7M3ryGE+ooi4BqqM7mp0QKL0jEtV1ISyOVq6ZgLSpmjnTbaacZ45M/bUV8B+AStyOx2qQ==" >> /etc/smbcredentials/onlineazurestorage.cred'
fi
sudo chmod 600 /etc/smbcredentials/onlineazurestorage.cred

sudo bash -c 'echo "//onlineazurestorage.file.core.windows.net/myfileshare /mnt/myfileshare cifs nofail,credentials=/etc/smbcredentials/onlineazurestorage.cred,dir_mode=0755,file_mode=0755,serverino,nosharesock,mfsymlinks,actimeo=30" >> /etc/fstab'
sudo mount -t cifs //onlineazurestorage.file.core.windows.net/myfileshare /mnt/myfileshare -o credentials=/etc/smbcredentials/onlineazurestorage.cred,dir_mode=0755,file_mode=0755,serverino,nosharesock,mfsymlinks,actimeo=30


